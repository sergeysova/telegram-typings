[ignore]
.*/node_modules/.*
.*/vscode/.*
.*/javascript/.*
.*/rust/.*
./.commitlintrc.js

[include]
./lib
./bin/telegram-typings

[libs]
./flow
./flow-typed

[lints]

[options]
all=true
traces=3
emoji=true
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
suppress_comment=\\(.\\|\n\\)*\\$FlowDisable

[strict]

